using Nemerle;
using Nemerle.Assertions;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace Nemerle.Compiler.Test
{
  /// <summary>
  /// Description of NccExecutable.
  /// </summary>
  public sealed class NccExecutable : ExternalNccRuntime
  {
    public this(nccPath : string)
      requires !string.IsNullOrEmpty(nccPath)
    {
      _nccPath = nccPath;
    }

    public override GetFileName() : string
    {
      _nccPath
    }

    public override GetArguments() : IEnumerable[string]
    {
      []
    }

    private _nccPath : string;
  }
}
